{
  "name": "cosc-499-project",
  "version": "1.0.0",
  "main": "src/main.js",
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "mime-types": "^3.0.1",
    "unzipper": "^0.12.3"
  },
  "scripts": {
    "test": "cross-env ELECTRON_RUN_AS_NODE=1 electron --test \"test/**/*.test.js\"",
    "detect:tech": "node src/lib/detectTechStack.js",
    "detect:tech:dry": "node src/lib/detectTechStack.js --dry",
    "start": "electron .",
    "dev": "electron .",
    "test:watch": "ELECTRON_RUN_AS_NODE=1 electron --test --watch test",
    "rebuild:electron": "npx electron-rebuild --force --module-dir .",
    "build:linux": "electron-builder -l",
    "build:win": "electron-builder -w",
    "build:mac": "electron-builder -m"
  },
  "build": {
    "appId": "com.yourteam.app",
    "files": [
      "**/*",
      "!dist",
      "!out",
      "!node_modules/.cache"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "cross-env": "^10.1.0",
    "electron": "^38.2.2",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "mock-fs": "^5.5.0"
  }
}
